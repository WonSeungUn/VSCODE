package kosa.io;

import java.io.FileOutputStream;
import java.io.ObjectOutputStream;

public class Main {

	Member m;
	
	// 按眉 流纺拳
	public void insert() {
		m = new Member("全辨悼", 20);
		ObjectOutputStream oos = null;
		
		try {
			oos = new ObjectOutputStream(new FileOutputStream("object.txt"));
			oos.writeObject(m);
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			try {
				oos.close();
			} catch (Exception e2) {

			}
		}
	}
	
	
	// 按眉 开流纺拳
	public void show() {
		
	}
	
	public static void main(String[] args) {
		Main main = new Main();
		main.insert();
		
	}
}
