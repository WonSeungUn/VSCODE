package kosa.thread;

public class SumThread extends Thread {
	private int start;
	private int end;
	private int total;
	
	
	public SumThread() {
	}
	
	
	public SumThread(int start, int end) {
		this.start = start;
		this.end = end;
	}


	@Override
	public void run() {

		for (int i = 1; i <= 50; i++) {
			t1 += i;
		}

		for (int i = 51; i <= 100; i++) {
			t2 += i;
		}
	}

