package kosa.io;

import java.io.FileReader;

public class KosaReader {
	public static void main(String[] args) {
		// 파일(poem.txt)에 잇는 문자열 데이터 읽어오기
		FileReader reader = null;
		
		
		try {
			// FileReader 생성
			reader = new FileReader("poem.txt");
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
